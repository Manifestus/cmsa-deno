{
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run --reload --allow-net --allow-env --allow-read --env-file=.env --watch --allow-ffi main.ts",
    "start": "deno run --allow-net --allow-env --allow-read --env-file=.env main.ts",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "seed": "deno run --allow-net --allow-env --allow-read --allow-ffi --env-file=.env ./prisma/seed.ts"
},
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.1.6",
    "@std/http": "jsr:@std/http@^1.0.7",
    "hono": "jsr:@hono/hono@^4.5.10",
    "jose": "npm:jose@^6.1.0",
    "prisma": "npm:prisma@^6.14.0",
    "@prisma/client": "npm:@prisma/client@^6.14.0",
    "@prisma/client/runtime/library": "npm:@prisma/client/runtime/library",
    "@prisma/client/wasm": "npm:@prisma/client/wasm",
    "@prisma/client/sql": "npm:@prisma/client/sql"
  },
  "prisma": {
    "seed": "deno run --allow-env --allow-read --allow-net --env-file=.env prisma/seed.ts"
  }
}
